<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Label?>
<VBox styleClass="secondary-pane" xmlns="http://javafx.com/javafx/11" xmlns:fx="http://javafx.com/fxml/1">
  <ListView fx:id="targetPersonView" VBox.vgrow="ALWAYS"/>

  <Label fx:id="reminderHeader" styleClass="target-person-header-2" text="Reminders" />

  <StackPane VBox.vgrow="ALWAYS">
    <ListView styleClass="reminder-view" fx:id="reminderView" VBox.vgrow="ALWAYS" minHeight="-Infinity" maxHeight="Infinity"/>
  </StackPane>
</VBox>
